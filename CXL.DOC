        /*...define window, begin of form, and all fields...*/
        winpkey(get_key,&key_that_terminated_form);
        winpread();
        switch(key_that_terminated_form) {
            case 0x4900:
                /* [PgUp] */
            case 0x5100:
                /* [PgDn] */
            case 0x1c0d:
            case 0x1c0a:
                /* [Enter] or [Ctrl-Enter] (aka [F10]) */
            case 0x011b:
                /* [Esc] */
        }

